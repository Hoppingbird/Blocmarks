<div class="row">
  <div class="col-md-8">
    <% @bookmarks.each do |bookmark| %>
        <div class="media">
          <div class="media-body">
            <h4 class="media-heading">
              <%= link_to bookmark.url, bookmark.url, :target => '_blank' %>
              <%= render partial: 'likes/like', locals: { topic: bookmark.topic, bookmark: bookmark } %>
            </h4>
          </div>
        </div>
    <% end %>
  </div>
</div>